<xsd:schema targetNamespace="http://www.EcoInvent.org/EcoSpold01"
	elementFormDefault="qualified" attributeFormDefault="unqualified"
	version="1.0" xmlns:es="http://www.EcoInvent.org/EcoSpold01"
	xmlns="http://www.EcoInvent.org/EcoSpold01"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">
	<xsd:annotation>
		<xsd:appinfo>
			<internalSchemaVersion>1.0</internalSchemaVersion>
		</xsd:appinfo>
	</xsd:annotation>
	<xsd:complexType name="TProcessInformation">
		<xsd:annotation>
			<xsd:documentation>
				Contains content-related metainformation for the unit
				process.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="referenceFunction"
				type="TReferenceFunction">
				<xsd:annotation>
					<xsd:documentation>
						Comprises information which identifies and
						characterises one particular dataset (=unit
						process or system terminated).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="geography" type="TGeography">
				<xsd:annotation>
					<xsd:documentation>
						Describes the geographic area for which the
						dataset is supposed to be valid. It is the
						supply region (not the production region) of the
						product service at issue. It helps the user to
						judge the geographical suitability of the
						process (or impact category) dataset for his or
						her application (purpose).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="technology" type="TTechnology">
				<xsd:annotation>
					<xsd:documentation>
						Describes the technological properties of the
						unit process. It helps the user to judge the
						technical suitability of the process dataset for
						his or her application (purpose).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="timePeriod" type="TTimePeriod">
				<xsd:annotation>
					<xsd:documentation>
						Characterises the temporal properties of the
						unit process (or system terminated) at issue. It
						helps the user to judge the temporal suitability
						of the process dataset for his or her
						application (purpose).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dataSetInformation"
				type="TDataSetInformation">
				<xsd:annotation>
					<xsd:documentation>
						Contains administrative information about the
						dataset (version number, language and
						localLanguage used).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TModellingAndValidation">
		<xsd:annotation>
			<xsd:documentation>
				Contains metaInformation about how unit processes are
				modelled and about the review/validation of the dataset.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="representativeness"
				type="TRepresentativeness" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Contains information about the
						representativeness of the unit process data
						(meta information and flow data).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="source" type="TSource"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Lists and describes the sources where the
						dataset is documented (final report in the
						ECOINVENT quality network series).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="validation" type="TValidation"
				minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>
						Contains information about the reviewers'
						comments on the dataset content.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TAdministrativeInformation">
		<xsd:annotation>
			<xsd:documentation>
				Contains information about the person that compiled and
				entered the dataset in the database and about kind of
				publication and the accessibility of the dataset.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="dataEntryBy" type="TDataEntryBy">
				<xsd:annotation>
					<xsd:documentation>
						Contains information about the person and the
						quality network the person belongs to.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dataGeneratorAndPublication"
				type="TDataGeneratorAndPublication">
				<xsd:annotation>
					<xsd:documentation>
						Contains information about the generator of the
						dataset in the database, whether the dataset has
						been published (and how) and about copyright and
						the accessibility of the dataset (public or
						restricted to ETH domain, ECOINVENT, or a
						particular institute of ECOINVENT.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="person" type="TPerson"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						Contains a list of persons with their addresses.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TMetaInformation">
		<xsd:annotation>
			<xsd:documentation>
				Contains information about the process (its name,
				(functional) unit, classification, technology,
				geography, time, etc.), about modelling assumptions and
				validation details and about dataset administration
				(version number, kind of dataset, language).
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="processInformation"
				type="TProcessInformation">
				<xsd:annotation>
					<xsd:documentation>
						Contains content-related metainformation for the
						unit process.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="modellingAndValidation"
				type="TModellingAndValidation">
				<xsd:annotation>
					<xsd:documentation>
						Contains metainformation about how unit
						processes are modelled and about the
						review/validation of the dataset.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="administrativeInformation"
				type="TAdministrativeInformation">
				<xsd:annotation>
					<xsd:documentation>
						Contains the administrative information about
						the dataset at issue: type of dataset (unit
						process, elementary flow, impact category,
						multi-output process) timestamp, version and
						internalVersion number as well as language and
						localLanguage code.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TFlowData">
		<xsd:annotation>
			<xsd:documentation>
				Contains information about inputs and outputs (to and
				from nature as well as to and from technosphere) and
				information about allocation (flows to be allocated,
				co-products to be allocated to, allocation factors).
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="exchange" type="TExchange"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						comprises all inputs and outputs (both
						elementary flows and intermediate product flows)
						registered in a unit process.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="allocation" type="TAllocation"
				minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						comprises all referenceToInputOutput.
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TDataset">
		<xsd:annotation>
			<xsd:documentation>
				contains information about one individual unit process
				(or terminated system). Information is divided into
				metaInformation and flowData.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="metaInformation"
				type="TMetaInformation">
				<xsd:annotation>
					<xsd:documentation>
						meta information contains information about the
						process (its name, (functional) unit,
						classification, technology, geography, time,
						etc.), about modelling assumptions and
						validation details and about dataset
						administration (version number, kind of dataset,
						language).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="flowData" type="TFlowData"
				maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>
						contains information about inputs and outputs
						(to and from nature as well as to and from
						technosphere) and information about allocation
						(flows to be allocated, co-products to be
						allocated to, allocation factors).
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:any namespace="##other" processContents="lax"
				minOccurs="0" maxOccurs="unbounded">
			</xsd:any>
		</xsd:sequence>
		<xsd:attribute name="number" type="TIndexNumber"
			use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>200</spoldID>
					<type>Number</type>
					<size>10</size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="internalSchemaVersion" type="xsd:string"
			use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>Number</type>
					<size></size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						No
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="generator" type="TString255"
			use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>String</type>
					<size>255</size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="timestamp" type="xsd:dateTime"
			use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>XML DateTime</type>
					<size>19</size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="validCompanyCodes" type="xsd:string"
			use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>String</type>
					<size></size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						No
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="validRegionalCodes" type="xsd:string"
			use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>String</type>
					<size></size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						No
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="validCategories" type="xsd:string"
			use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>String</type>
					<size></size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						No
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="validUnits" type="xsd:string"
			use="optional">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID></spoldID>
					<type>String</type>
					<size></size>
					<options></options>
					<multipleOccurences>No</multipleOccurences>
					<valueRequiredInEcoInvent>
						No
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	<xsd:element name="ecoSpold">
		<xsd:complexType>
			<xsd:annotation>
				<xsd:documentation>
					the data (exchange) format of the ECOINVENT quality
					network.
				</xsd:documentation>
			</xsd:annotation>
			<xsd:sequence>
				<xsd:element name="dataset" type="TDataset"
					maxOccurs="unbounded">
					<xsd:annotation>
						<xsd:documentation>
							a dataset describes LCI related information
							of a unit process or a terminated system
							comprising metaInformation (description of
							the process) and flowData (quantified inputs
							and outputs and allocation factors, if any).
						</xsd:documentation>
					</xsd:annotation>
					<xsd:unique name="pkPersonNumber">
						<xsd:selector
							xpath="es:metaInformation/es:administrativeInformation/es:person">
						</xsd:selector>
						<xsd:field xpath="@number"></xsd:field>
					</xsd:unique>
					<xsd:unique name="pkSourceNumber">
						<xsd:selector
							xpath="es:metaInformation/es:modellingAndValidation/es:source">
						</xsd:selector>
						<xsd:field xpath="@number"></xsd:field>
					</xsd:unique>
					<xsd:unique name="pkExchangeNumber">
						<xsd:selector xpath="es:flowData/es:exchange"></xsd:selector>
						<xsd:field xpath="@number"></xsd:field>
					</xsd:unique>
					<xsd:keyref name="fkDataEntryByPerson"
						refer="pkPersonNumber">
						<xsd:selector
							xpath="es:metaInformation/es:administrativeInformation/es:dataEntryBy">
						</xsd:selector>
						<xsd:field xpath="@person"></xsd:field>
					</xsd:keyref>
					<xsd:keyref name="fkDataGenerationAndPublication"
						refer="pkPersonNumber">
						<xsd:selector
							xpath="es:metaInformation/es:administrativeInformation/es:dataGeneratorAndPublication">
						</xsd:selector>
						<xsd:field xpath="@person"></xsd:field>
					</xsd:keyref>
					<xsd:keyref
						name="fkDataGenerationAndPublicationSource"
						refer="pkSourceNumber">
						<xsd:selector
							xpath="es:metaInformation/es:administrativeInformation/es:dataGeneratorAndPublication">
						</xsd:selector>
						<xsd:field
							xpath="@referenceToPublishedSource">
						</xsd:field>
					</xsd:keyref>
					<xsd:keyref name="fkAllocationExchange"
						refer="pkExchangeNumber">
						<xsd:selector
							xpath="es:flowData/es:allocation">
						</xsd:selector>
						<xsd:field xpath="@referenceToCoProduct"></xsd:field>
					</xsd:keyref>
					<xsd:keyref name="fkAllocationExchangeInOut"
						refer="pkExchangeNumber">
						<xsd:selector
							xpath="es:flowData/es:allocation/es:referenceToInputOutput">
						</xsd:selector>
						<xsd:field xpath="."></xsd:field>
					</xsd:keyref>
					<xsd:keyref name="fkValidationPerson"
						refer="pkPersonNumber">
						<xsd:selector
							xpath="es:metaInformation/es:modellingAndValidation/es:validation">
						</xsd:selector>
						<xsd:field xpath="@proofReadingValidator"></xsd:field>
					</xsd:keyref>
					<xsd:keyref name="fkExchangeSource"
						refer="pkSourceNumber">
						<xsd:selector xpath="es:flowData/es:exchange"></xsd:selector>
						<xsd:field xpath="@referenceToSource"></xsd:field>
					</xsd:keyref>
				</xsd:element>
				<xsd:any namespace="##other" processContents="lax"
					minOccurs="0" maxOccurs="unbounded">
				</xsd:any>
			</xsd:sequence>
			<xsd:attribute name="validationId" type="xsd:integer"
				use="optional">
			</xsd:attribute>
			<xsd:attribute name="validationStatus" type="xsd:string"
				use="optional">
			</xsd:attribute>
		</xsd:complexType>
		<xsd:unique name="pkIndexNumber">
			<xsd:selector xpath="es:dataset"></xsd:selector>
			<xsd:field xpath="@number"></xsd:field>
		</xsd:unique>
	</xsd:element>
	<xsd:simpleType name="TIndexNumber">
		<xsd:annotation>
			<xsd:documentation>
				Unique identifier. Format: Integer (32-bit) greater then
				zero
			</xsd:documentation>
			<xsd:documentation>
				Eindeutige Nummer zur Identifizierung eines Prozesses.
				Format: Integer (32-bit) groesser 0
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:int">
			<xsd:minInclusive value="1" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TPercent">
		<xsd:annotation>
			<xsd:documentation>Format: ddd.d</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:float">
			<xsd:pattern value="\d{1,3}\.\d" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TUnit">
		<xsd:annotation>
			<xsd:documentation>20 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TFloatNumber">
		<xsd:annotation>
			<xsd:documentation>
				Floating Point Zahl, 64-bit Genauigkeit, mit Vorzeichen,
				zur Angabe von Mengen und Unsicherheiten
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:double" />
	</xsd:simpleType>
	<xsd:simpleType name="TString20">
		<xsd:annotation>
			<xsd:documentation>20 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="20" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString30">
		<xsd:annotation>
			<xsd:documentation>30 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="30" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString40">
		<xsd:annotation>
			<xsd:documentation>40 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="40" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString80">
		<xsd:annotation>
			<xsd:documentation>80 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="80" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString255">
		<xsd:annotation>
			<xsd:documentation>255 letter string.</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="255" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TString32000">
		<xsd:annotation>
			<xsd:documentation>
				Free text for additional remarks, detailed descriptions
				etc.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="32000" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TCompanyCode">
		<xsd:annotation>
			<xsd:documentation>
				7 letter company code (capital letters).
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="7" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TRegionalCode">
		<xsd:annotation>
			<xsd:documentation>
				7 letter regional code (capital letters). Enlarged list
				of 2 letter ISO country codes by codes for regions,
				continents, market areas and organisations and
				companies.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:maxLength value="7" />
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="TPerson">
		<xsd:annotation>
			<xsd:documentation>
				Used for the identification of members of the
				organisation / institute co-operating within a quality
				network (e.g., ecoinvent) referred to in the areas
				Validation, dataEntryBy and dataGeneratorAndPublication.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:attribute name="number" type="TIndexNumber"
			use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5800</spoldID>
					<type>Number (Integer)</type>
					<size>10</size>
					<options />
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>
					ID number is attributed to each person of an
					organisation/institute co-operating in a quality
					network such as ecoinvent. It is used to identify
					persons cited within one dataset.
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="name" type="TString40" use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5802</spoldID>
					<type>Text</type>
					<size>40</size>
					<options />
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>
					Name and surname of the person working in an
					organisation/institute which is a member of the
					quality network.
				</xsd:documentation>
				<xsd:documentation>
					Identifies the person together with 'address'
					(#5803).
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
		<xsd:attribute name="address" type="TString255"
			use="required">
			<xsd:annotation>
				<xsd:appinfo>
					<spoldID>5803</spoldID>
					<type>Text</type>
					<size>255</size>
					<options />
					<multipleOccurences>Yes</multipleOccurences>
					<valueRequiredInEcoInvent>
						Yes
					</valueRequiredInEcoInvent>
				</xsd:appinfo>
				<xsd:documentation>
					Complete address, including street, po-box (if
					applicable), zip-code, city, state (if applicable),
